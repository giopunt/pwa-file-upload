{
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "plugin:prettier/recommended",
    "prettier/react"
  ],
  "plugins": [
    "import",
    "prettier",
    "jsx-a11y"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "linebreak-style": "off",
    "prettier/prettier": "error"
  }
}
